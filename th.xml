def GetLSProData(page_data,Cookie_Jar,m,cid="tnn16"):
  import re, requests
  from requests.auth import HTTPBasicAuth
  s = requests.session()
  url = "https://tv.trueid.net/embed/{0}".format(cid)
  s.headers = {'User-Agent': 'Mozilla/5.0 (Windows; U; Windows NT 5.1; en-GB; rv:1.9.0.3) Gecko/2008092417 Firefox/3.0.3'}
  page = s.get(url)
  user, passwd = re.findall(r'username":"([^"]+)","password":"([^"]+)', page.text)[0]
  ccode = re.findall(r'channel_code":"([^"]+)', page.text)[0]
  apiurl = "https://tv.trueid.net/apis/player"
  s.headers.update({'Origin': 'https://tv.trueid.net', 'Referer': 'https://tv.trueid.net/'})
  data = {"ssoId":"nologin","channel_code":ccode,"visitor":"web","drm":"aes","lang":"th","concurrent":"no"}
  spage = s.post(apiurl, json=data, auth=HTTPBasicAuth(user, passwd))
  strurl = spage.json().get("stream").get("result")
  return strurl
]]></expres>
<page></page>
</regex>
