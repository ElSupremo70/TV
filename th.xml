  ccode = re.findall(r'channel_code":"([^"]+)', page.text)[0]
  apiurl = "https://tv.trueid.net/apis/player"
  s.headers.update({'Origin': burl[:-1], 'Referer': burl})
  data = {"ssoId":"27086721","channel_code":ccode,"visitor":"web","drm":"wv","lang":"th","concurrent":"no"}
  spage = s.post(apiurl, json=data, auth=HTTPBasicAuth(user, passwd)).json()
